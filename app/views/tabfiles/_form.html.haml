= form_for @tabfile do |f|
  - if @tabfile.errors.any?
    #error_explanation
      %h2= "#{pluralize(@tabfile.errors.count, "error")} prohibited this tabfile from being saved:"
      %ul
        - @tabfile.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :file_name
    = f.file_field :file_name
  .actions
    = f.submit 'Save'
